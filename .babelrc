{
	"presets" : ["es2015", "react",
		["env", {
			"targets": {
				"browsers": ["Explorer 11"]
			}
		}]],
	"plugins": [
		["babel-plugin-root-import",
			[{
				"rootPathPrefix": "~",
				"rootPathSuffix": "source/js"
			},
				{
					"rootPathPrefix": "@",
					"rootPathSuffix": "source/data"
				},
				{
					"rootPathPrefix": "#",
					"rootPathSuffix": "source/md"
				}
			]
		]
	]
}